        model="queue.job.function"
    >
        <field
            name="model_id"
            ref="connector_pms_wubook.model_channel_wubook_product_pricelist"
        />
        <field name="method">import_record</field>
        <field name="channel_id" ref="connector_pms_wubook.channel_wubook" />
        <field
            name="related_action"
