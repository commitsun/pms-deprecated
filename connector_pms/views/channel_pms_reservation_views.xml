<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="channel_pms_reservation_view_form" model="ir.ui.view">
        <field name="name">channel.pms.reservation.form</field>
        <field name="model">channel.pms.reservation</field>
        <field name="arch" type="xml">
            <form string="Pms Channel Reservation">
                <group>
                    <field name="external_id" />
                    <field name="ota_reservation_id" />
                    <field name="channel_status" />
                    <field name="channel_status_reason" />
                    <field name="to_assign" />
                </group>
            </form>
        </field>
    </record>
    <record id="view_channel_pms_reservation_tree" model="ir.ui.view">
        <field name="name">channel.pms.reservation.tree</field>
        <field name="model">channel.pms.reservation</field>
        <field name="arch" type="xml">
            <tree string="Pms Channel Reservation">
                <field name="backend_id" />
            </tree>
        </field>
    </record>
</odoo>
